{
  "OnDestroy Subject": {
    "prefix": "@fs/destroy",
    "body": [
      "private _destroy$ = new Subject();",
      "",
      "public ngOnDestroy() {",
      "  this._destroy$.next();",
      "  this._destroy$.complete();",
      "}"
    ]
  },
  "Subscribe": {
    "prefix": "@fs/subscribe",
    "body": [
      "  .pipe(",
      "    takeUntil(this._destroy$)",
      "  )",
      "  .subscribe(response => {",
      "  ",
      "  });"
    ]
  },
  "Console Log": {
    "prefix": "@fs/console",
    "body": [
      "console.log($1)"
    ]
  }
}