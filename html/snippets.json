{
  "Material Button": {
    "prefix": "@fs/button",
    "body": [
      "<button",
      "    ${1:mat-raised-button}",
      "    color=\"primary\">",
      "  $2",
      "</button>"
    ]
  },
  "Material Select": {
    "prefix": "@fs/select",
    "body": [
      "<mat-form-field>",
      "  <mat-label>${1/(^.|_.|\\..)/${1:/upcase}/g}</mat-label>",
      "  <mat-select",
      "      [(ngModel)]=\"$1\"",
      "      name=\"${1/.*?([^.]+)$/$1/}\">",
      "    <mat-option",
      "        *ngFor=\"let item of items\"",
      "        [value]=\"item.id\">",
      "      {{ item.name }}",
      "    </mat-option>",
      "  </mat-select>",
      "</mat-form-field>"
    ]
  },
  "Material Input": {
    "prefix": "@fs/input",
    "body": [
      "<mat-form-field>",
      "  <mat-label>${1/(^.|_.|\\..)/${1:/upcase}/g}</mat-label>",
      "  <input",
      "    matInput",
      "    [(ngModel)]=\"$1\"",
      "    name=\"${1/.*?([^.]+)$/$1/}\">",
      "</mat-form-field>"
    ]
  },
  "Material Textarea": {
    "prefix": "@fs/textarea",
    "body": [
      "<mat-form-field>",
      "  <mat-label>${1/(^.|_.|\\..)/${1:/upcase}/g}</mat-label>",
      "  <textarea",
      "    matInput",
      "    cdkTextareaAutosize",
      "    [(ngModel)]=\"$1\"",
      "    name=\"${1/.*?([^.]+)$/$1/}\">",
      "  </textarea>",
      "</mat-form-field>"
    ]
  },
  "Firestitch Radio Button Group": {
    "prefix": "@fs/radio-group",
    "body": [
      "<fs-radio-group",
      "    [(ngModel)]=\"$1\"",
      "    label=\"${1/(^.|_.|\\..)/${1:/upcase}/g}\"",
      "    name=\"${1/.*?([^.]+)$/$1/}\"",
      "    orientation=\"${2:horizontal}\">",
      "  <mat-radio-button",
      "      *ngFor=\"let item of items\" [value]=\"item.id\">",
      "    {{ item.value }}",
      "  </mat-radio-button>",
      "</fs-radio-group>"
    ]
  },
  "Firestitch Checkbox Group": {
    "prefix": "@fs/checkbox-group",
    "body": [
      "<fs-checkbox-group",
      "    [(ngModel)]=\"$1\"",
      "    label=\"${1/(^.|_.|\\..)/${1:/upcase}/g}\"",
      "    name=\"${1/.*?([^.]+)$/$1/}\"",
      "    orientation=\"${2:horizontal}\">",
      "  <mat-checkbox",
      "      *ngFor=\"let item of items\" [value]=\"item.name\">",
      "    {{ item.value }}",
      "  </mat-checkbox>",
      "</fs-checkbox-group>"
    ]
  },
  "Firestitch Date Picker": {
    "prefix": "@fs/date-picker",
    "body": [
      "<mat-form-field>",      
      "  <mat-label>${1/(^.|_.|\\..)/${1:/upcase}/g}</mat-label>",
      "  <input",
      "    matInput",
      "    fsDatePicker",
      "    [(ngModel)]=\"$1\"",
      "    name=\"${1/.*?([^.]+)$/$1/}\">",
      "</mat-form-field>"
    ]
  },
  "Firestitch Autocomplete": {
    "prefix": "@fs/autocomplete",
    "body": [
      "<fs-autocomplete",
      "    [fetch]=\"fetch\"",
      "    [fetchOnFocus]=\"false\"",
      "    [displayWith]=\"displayWith\"",
      "    [(ngModel)]=\"$1\"",
      "    placeholder=\"${1/(^.|_.|\\..)/${1:/upcase}/g}\"",
      "    name=\"${1/.*?([^.]+)$/$1/}\">",
      "  <ng-template fsAutocompleteTemplate let-data=\"data\">",
      "    {{ data.name }}",
      "  </ng-template>",
      "  <ng-template fsAutocompleteStaticTemplate>",
      "    <div (click)=\"create($event)\">Create</div>",
      "  </ng-template>",
      "</fs-autocomplete>"
    ]
  },
  "Firestitch List": {
    "prefix": "@fs/list",
    "body": [
      "<fs-list [config]=\"listConfig\">",
      "  <fs-list-column title=\"$1\" name=\"${1/(.*)/${1:/downcase}/g}\">",
      "    <ng-template fs-list-cell let-row=\"row\">",
      "      {{row}}",
      "    </ng-template>",
      "  </fs-list-column>",
      "</fs-list>"
    ]
  },
  "Firestitch Label Field": {
    "prefix": "@fs/label-field",
    "body": [
      "<fs-label-field>",
      "  <fs-label>$1</fs-label>",
      "  $2",
      "</fs-label-field>"
    ]
  },
  "Flex Row": {
    "prefix": "@fs/row",
    "body": [
      "<div",
      "  fxLayout=\"row\"",
      "  fxLayoutAlign=\"${1:start} ${2:center}\">",
      "    $3",
      "</div>"
    ]
  },
  "Flex Column": {
    "prefix": "@fs/column",
    "body": [
      "<div",
      "  fxLayout=\"column\">",
      "    $1",
      "</div>"
    ]
  },
  "Firestitch Dialog": {
    "prefix": "@fs/dialog",
    "body": [
      "<form fsForm [submit]=\"save\">",
      "  <fs-dialog>",
      "    <h1 mat-dialog-title>$1</h1>",
      "    <mat-dialog-content>",
      "    </mat-dialog-content>",
      "    <mat-dialog-actions>",
      "      <fs-form-dialog-actions",
      "        [create]=\"!$2.id\">",
      "      </fs-form-dialog-actions>",
      "      <!--",
      "      <button",
      "          type=\"button\"",
      "          mat-button",
      "          [mat-dialog-close]=\"null\">",
      "        Cancel",
      "      </button>",
      "      -->",
      "    </mat-dialog-actions>",
      "  </fs-dialog>",
      "</form>"
    ]
  }
}


